<?xml version="1.0" encoding="utf-8"?>
<Project>
  <!--
    We need to sign all emsdk files with the 3rd party certificate instead of the Microsoft400 one.
  -->
  <ItemGroup>
   <FileExtensionSignInfo Condition="'%(FileExtensionSignInfo.CertificateName)' == 'Microsoft400'">
      <CertificateName>3PartySHA2</CertificateName>
   </FileExtensionSignInfo>
  </ItemGroup>
</Project>
